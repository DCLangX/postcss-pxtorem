{
  "name": "@minko-fe/postcss-pxtorem",
  "version": "0.0.2",
  "description": "âœ¨ A CSS post-processor that converts px to rem.",
  "main": "dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "README-en.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/any-config.git"
  },
