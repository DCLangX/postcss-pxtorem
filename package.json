  "type": "module",
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "license": "MIT",
  "dependencies": {
    "@stdlib/regexp-regexp": "^0.0.8",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@minko-fe/commitlint-config": "^1.1.7",
    "@minko-fe/eslint-config-ts": "^1.1.7",
    "@minko-fe/prettier-config": "^1.1.7",
    "@minko-fe/tsconfig": "^1.1.7",
    "eslint": "^8.22.0",
    "postcss": "^8.4.16",
